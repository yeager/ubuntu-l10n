#!/usr/bin/env python3
import sys
import os
sys.path.insert(0, '/usr/lib/python3/dist-packages')

try:
    from ubuntu_l10n.main import main
    main()
except ImportError:
    try:
        from ubuntu_l10n.app import main
        main()
    except ImportError:
        print("Error: Cannot find main module for ubuntu-l10n", file=sys.stderr)
        sys.exit(1)
