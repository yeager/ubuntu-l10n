#!/usr/bin/env python3
import sys
import os
sys.path.insert(0, '/usr/lib/python3/dist-packages')

app_name = os.path.basename(sys.argv[0])
module_name = app_name.replace('-', '_')

try:
    exec(f"from {module_name}.main import main; main()")
except ImportError:
    try:
        exec(f"from {module_name}.app import main; main()")
    except ImportError:
        print(f"Error: Cannot find main module for {app_name}", file=sys.stderr)
        sys.exit(1)
